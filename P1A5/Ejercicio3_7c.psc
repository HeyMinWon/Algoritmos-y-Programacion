	//Ana Renata Rojas Santillán
	//Ing. en Animación digital
	Definir B, C, D como enteros;
	Escribir "Intruduzca el valor de B: ";
	Leer B;
	Escribir "Introduzca el valor de C: ";
	Leer C;
	Escribir "Introduzca el valor de D: ";
	Leer D;
	Escribir "(B+D)/(C+4)= ", (B+D)/(C+4);
